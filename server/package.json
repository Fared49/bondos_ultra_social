{
  "name": "bondos-ultra-social-server",
  "version": "2.0.0",
  "description": "Realtime social platform with games, rooms, chat",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --coverage --passWithNoTests"
  },
  "keywords": ["social", "realtime", "games", "rooms"],
  "author": "Bondos Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "mongoose": "^7.6.0",
    "dotenv": "^16.4.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.1.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.0.0",
    "supertest": "^6.3.3",
    "mongodb-memory-server": "^8.12.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --coverage --passWithNoTests",
    "lint": "eslint src --ext .js"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.cjs"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
